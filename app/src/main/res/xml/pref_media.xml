        android:title="@string/pref_media_match_mode"
        android:summary="@string/pref_media_match_mode_summary"
        android:entries="@array/pref_media_match_mode_entrie_array"
        android:entryValues="@array/pref_media_match_mode_entrie_values"
        android:defaultValue="@string/pref_media_match_mode_entrie_default"
